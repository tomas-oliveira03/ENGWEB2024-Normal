extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
        h1 Contrato #{contrato.idcontrato}

    p Número de Anúncio: #{contrato.nAnuncio}
    p Tipo de Procedimento: #{contrato.tipoprocedimento}
    p Objeto Contrato: #{contrato.objectoContrato}
    p Data de publicação: #{contrato.dataPublicacao}
    p Data de Celebração do Contrato: #{contrato.dataCelebracaoContrato}
    p Preço Contratual: #{contrato.precoContratual}
    p Prazo de Execução: #{contrato.prazoExecucao}
    p NIPC Entidade Comunicante: #{contrato.NIPC_entidade_comunicante}
    p Entidade Comunicante: #{contrato.entidade_comunicante}
    p Fundamentação: #{contrato.entidade_comunicante}

    footer.w3-container.w3-blue
        h5
            span Generated by EngWeb2024 in #{date} - [
                a(href='/') Return
            span ]

